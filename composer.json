{
  "name": "emdeevy/base32",
  "type": "library",
  "description": "Base32 encoder/decoder according to RFC 4648",
  "license": "MIT",
  "keywords": ["base32","encode","decode","rfc4648"],
  "homepage": "https://github.com/emdeevy/otp/tree/php",
  "authors": [
    {
      "name": "Denis Makula",
      "homepage": "https://github.com/emdeevy",
      "email": "denis@emdeevy.me",
      "role": "Developer"

    }
  ],
  "require": {
    "php": "^7.2 || ^8.0"
  },
  "autoload": {
    "psr-4": { "emdeevy\\base32\\": "src/" }
  },
  "autoload-dev": {
    "psr-4": { "emdeevy\\base32\\test\\": "test/" }
  },
  "config": {
    "preferred-install": {
      "*": "dist"
    },
    "sort-packages": true
  }
}